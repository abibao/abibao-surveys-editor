{
  "name": "abibao-surveys-editor",
  "version": "1.1.6",
  "description": "Frontend surveys editor",
  "author": "Gilles Perreymond",
  "scripts": {
    "build": "react-scripts build",
    "test": "./npm-scripts/test.sh",
    "test:react": "react-scripts test --env=jsdom",
    "test:standard": "node_modules/.bin/standard --verbose | snazzy",
    "test:coverage": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --opts .mocharc",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:server": "DEBUG=feathers* node server/start.js"
  },
  "dependencies": {
    "aws-sdk": "2.12.0",
    "body-parser": "1.16.1",
    "compression": "1.6.2",
    "cors": "2.8.1",
    "feathers": "2.0.3",
    "feathers-authentication": "1.0.2",
    "feathers-authentication-jwt": "0.3.1",
    "feathers-authentication-local": "0.3.3",
    "feathers-blob": "1.2.0",
    "feathers-configuration": "0.4.1",
    "feathers-hooks": "1.7.1",
    "feathers-legacy-authentication-hooks": "1.0.0",
    "feathers-memory": "1.1.0",
    "feathers-rest": "1.6.0",
    "feathers-sequelize": "1.4.0",
    "feathers-socketio": "1.4.2",
    "fs-blob-store": "5.2.1",
    "lodash": "4.17.4",
    "nconf": "0.8.4",
    "pg": "6.1.2",
    "pg-hstore": "2.3.2",
    "sequelize": "3.30.2",
    "serve-favicon": "2.3.2",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "codacy-coverage": "2.0.1",
    "eslint-config-standard": "7.0.0-beta.0",
    "eslint-config-standard-react": "4.2.0",
    "eslint-plugin-promise": "3.4.1",
    "eslint-plugin-react": "6.9.0",
    "eslint-plugin-standard": "2.0.1",
    "feathers-authentication-client": "0.1.8",
    "istanbul": "0.4.5",
    "material-ui": "0.17.0",
    "mocha": "3.2.0",
    "mocha-standard": "1.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-dropzone": "3.10.0",
    "react-grid-system": "2.4.1",
    "react-markdown": "2.4.4",
    "react-router": "3.0.2",
    "react-scripts": "0.9.0",
    "react-tap-event-plugin": "2.0.1",
    "reflux": "6.2.0",
    "snazzy": "5.0.0",
    "standard-jsx": "8.0.0",
    "survey-react": "0.11.1"
  },
  "standard": {
    "globals": [
      "describe",
      "it"
    ],
    "ignore": [
      "build/**",
      "coverage/**",
      "public/**"
    ]
  }
}
