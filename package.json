{
  "name": "abibao-surveys-editor",
  "version": "1.4.0",
  "description": "Frontend surveys editor",
  "author": "Gilles Perreymond",
  "scripts": {
    "build": "react-scripts build",
    "test": "NODE_ENV=test && ./npm-scripts/test.sh",
    "test:react": "react-scripts test --env=jsdom",
    "test:standard": "node_modules/.bin/standard --verbose | snazzy",
    "test:unit": "node_modules/.bin/_mocha --recursive test/unit",
    "test:coverage": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --opts .mocharc",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:server": "NODE_ENV=development node_modules/.bin/nodemon --config nodemon.json server/start.js",
    "start:server:debug": "DEBUG=feathers* node server/start.js"
  },
  "proxy": "http://localhost:3000",
  "dependencies": {
    "bluebird": "3.5.0",
    "body-parser": "1.17.1",
    "boom": "4.3.1",
    "bunyan": "1.8.10",
    "bunyan-logstash-tcp": "0.3.5",
    "compression": "1.6.2",
    "cors": "2.8.3",
    "cryptr": "2.0.0",
    "eraro": "0.4.1",
    "feathers": "2.1.1",
    "feathers-authentication": "1.2.2",
    "feathers-authentication-jwt": "0.3.1",
    "feathers-authentication-local": "0.3.4",
    "feathers-blob": "1.2.0",
    "feathers-configuration": "0.4.1",
    "feathers-errors": "2.7.0",
    "feathers-hooks": "2.0.0",
    "feathers-logger": "0.2.3",
    "feathers-memory": "1.1.0",
    "feathers-permissions": "0.1.1",
    "feathers-rest": "1.7.2",
    "feathers-sequelize": "1.4.5",
    "feathers-socketio": "1.6.0",
    "fs-blob-store": "5.2.1",
    "joi": "10.4.1",
    "lodash": "4.17.4",
    "nconf": "0.8.4",
    "node-couchdb": "1.1.1",
    "pg": "6.1.5",
    "pg-hstore": "2.3.2",
    "request": "2.81.0",
    "request-promise": "4.2.0",
    "sendgrid": "5.0.1",
    "sequelize": "3.30.4",
    "serve-favicon": "2.4.2",
    "servicebus": "2.0.10",
    "servicebus-retry": "0.0.11",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "codacy-coverage": "2.0.2",
    "colors": "1.1.2",
    "debug": "2.6.4",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-standard": "7.1.0",
    "eslint-config-standard-jsx": "3.3.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-standard": "2.1.1",
    "feathers-authentication-client": "0.3.1",
    "is_js": "0.9.0",
    "istanbul": "0.4.5",
    "mocha": "3.3.0",
    "mocha-standard": "1.0.0",
    "nodemon": "1.11.0",
    "pouchdb": "6.2.0",
    "progress": "2.0.0",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-cookie": "1.0.4",
    "react-dom": "15.4.2",
    "react-dropzone": "3.12.2",
    "react-markdown": "2.4.6",
    "react-router": "3.0.2",
    "react-scripts": "0.9.5",
    "reflux": "6.4.1",
    "request": "2.81.0",
    "semantic-ui-css": "2.2.10",
    "semantic-ui-react": "0.68.0",
    "sinon": "2.1.0",
    "snazzy": "7.0.0",
    "standard": "10.0.2",
    "standard-jsx": "8.0.0",
    "survey-react": "0.12.8"
  },
  "standard": {
    "globals": [
      "before",
      "after",
      "describe",
      "it",
      "expect"
    ],
    "ignore": [
      "build/**",
      "coverage/**",
      "public/**"
    ]
  }
}
