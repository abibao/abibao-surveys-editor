{
  "name": "abibao-surveys-editor",
  "version": "2.2.0",
  "description": "Abibao surveys platform.",
  "author": "Gilles Perreymond",
  "scripts": {
    "test": "./test/run.sh",
    "test:standard": "node_modules/.bin/standard --verbose | snazzy",
    "test:server": "node_modules/.bin/nyc --report-dir coverage-server --clean -r html -r lcovonly -r text node_modules/.bin/mocha --opts .mocharc",
    "test:client": "cross-env BABEL_ENV=test node_modules/.bin/karma start karma.conf.js",
    "start:dashboard": "./node_modules/.bin/webpack --watch",
    "start:server": "DEBUG=feathers:* node_modules/.bin/nodemon --config nodemon.json server/start.js",
    "build:dashboard": "NODE_ENV=protuction ./node_modules/.bin/webpack --colors --config=webpack.config.prod.js",
    "docker:start:server": "docker-compose exec platform-api npm run start:server"
  },
